{
  "name": "Bootstrapify",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "http://www.bootstrapcdn.com/"
  ],
  "background": {
    "scripts": ["startup.js", "bootstrapify.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Import Bootstrap Styles"
  },
  "manifest_version": 2
}